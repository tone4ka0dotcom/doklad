   "source": [
    "import difflib\n",
    "s1 = u'А роза упала на лапу Азора'\n",
    "s2 = u'Дай, Джим, на счастье лапу мне'\n",
    "sm = difflib.SequenceMatcher(lambda c: c in set(' ,'), s1, s2)\n",
    "m = sm.find_longest_match(0, len(s1), 0, len(s2))\n",
    "s1[m.a:m.b]\n"
   ]
  }
